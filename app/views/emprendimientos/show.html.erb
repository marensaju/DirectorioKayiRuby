<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">


<!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

<%= stylesheet_link_tag "bootstrap-social" %>


<script src="https://use.fontawesome.com/135bfc18e0.js"></script>


<p id="notice"><%= notice %></p>


<div class="wrapper">
    <div class="header header-filter" >
      
  <%= image_tag @emprendimiento.portada.url(:medium), :class=>"img-responsive"%>
    </div>

    <div class="main main-raised">
      <div class="profile-content">
              <div class="container">
                  <div class="row">
                      <div class="profile">
                          <div class="avatar ">

  <%= image_tag @emprendimiento.logo.url(:thumb)%>

                              
                          </div>
                          <div class="name">
                              <h3 class="title"><%= @emprendimiento.titulo %></h3>
               
                          </div>
                      </div>
                  </div>
                  <div class="description text-center">
                        <p> <%= @emprendimiento.descripcion %> </p>
                  </div>

          <div class="row">
            <div class="col-md-6 col-md-offset-3">
              <div class="profile-tabs">
                <div class="nav-align-center">
                  <ul class="bg-primary" >

                    <li >
                      <p >
                       
                        Datos Generales
                      </p>

                      
                    </li>
                   
                            </ul>

                            <div class="tab-content gallery">
                    <div class="tab-pane active" >
                                    <div class="row">
                        <div class="col-md-6 ">
                        <p >
                        <strong >Correo electronico:</strong>
                        <%= @emprendimiento.correo %>
                        </p>

                      <p>
                        <strong>Página web:</strong>
                       
                       <%= link_to @emprendimiento.pweb,@emprendimiento.pweb , data: { confirm: "Estas seguro?" }, :style=>"color:blue"%>
                      </p>

                      <p>
                        <strong>Horario de atención:</strong>
                        <%= @emprendimiento.ihoraio %> -
                        <%= @emprendimiento.fhorario %>
                      </p>

                      

                      <p>
                        <strong>Teléfono:</strong>
                        <%= @emprendimiento.telefono %>
                      </p>

                      <p>
                        <strong>Dirección:</strong>
                        <%= @emprendimiento.direccion %>
                      </p>
                      <p>
                        <strong>
                         Redes Sociales
                      </strong>
                      </p>
                      
                    
                    
                      <a class="btn btn-social-icon btn-twitter" href="<%= @emprendimiento.ptwitter %>" >
                          <span class="fa fa-twitter" style="color:white"></span>
                        </a>        
                      <a class="btn btn-social-icon btn-facebook" href="<%= @emprendimiento.pface %>" >
                          <span class="fa fa-facebook" style="color:white"></span>
                        </a>

                      <a class="btn btn-social-icon btn-instagram" href=" <%= @emprendimiento.pinstagram %>" >
                          <span class="fa fa-instagram" style="color:white"></span>
                        </a>

                      <a class="btn btn-social-icon btn-pinterest" href="<%= @emprendimiento.pyoutube %>" >
                          <span class="fa fa-youtube" style="color:white"></span>
                        </a>

                      <a class="btn btn-social-icon btn-linkedin" href="<%= @emprendimiento.plinkedin %>" >
                          <span class="fa fa-linkedin" style="color:white"></span>
                        </a>

                      <a class="btn btn-social-icon btn-github" href="<%= @emprendimiento.pbehance %>" >
                          <span class="fa fa-behance" style="color:white"></span>
                        </a>
                         
                        </div>
                        <div class="col-md-6">

                        <p>
                        <strong>Video:</strong>

                        <%= link_to @emprendimiento.lvideo,@emprendimiento.lvideo , data: { confirm: "Estas seguro?" }, :style=>"color:blue"%>
                        

                    
                        
                      </p>


                      <div class="field">

                        <%= image_tag @emprendimiento.producto1.url(:thumb)%>
                      </div>


    








                         
                        </div>
                                    </div>
                                </div>
                                
                    

                            </div>
                </div>
              </div>
              <!-- End Profile Tabs -->
            </div>
                  </div>

              </div>
          </div>
    </div>

    </div>























