        <div class="col-lg-10">
        <%= f.text_field :plinkedin, :class=>"form-control" ,:placeholder=>"Url linkedin" %>
        </div>
      </div>
    </div>





</fieldset>
</div>


  </div>
  <div class="col-md-6">
