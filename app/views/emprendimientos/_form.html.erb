        <%= f.text_field :lvideo, :class=>"form-control" ,:placeholder=>"Url video promocional" %><br>
        </div>
      </div>
    </div>




<div class="row form-group ">
<div class="field">
<div class=" col-lg-2 ">
 <%= f.label :portada, "Imagen de portada",:class=>" control-label" %>
</div>
