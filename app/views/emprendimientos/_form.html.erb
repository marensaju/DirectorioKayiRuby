<%= stylesheet_link_tag "Spacelab" %>

<%= form_for(@emprendimiento) do |f|  %>
 

<div class="container">
 <% if @emprendimiento.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@emprendimiento.errors.count, "error") %> prohibited this emprendimiento from being saved:</h2>

      <ul>
      <% @emprendimiento.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<div class="row">

<div class="form-horizontal">
   <fieldset>
    <legend>Datos Generales</legend>
  <div class="col-md-6">
  <div class="form-group">
      <div class="field">
        <%= f.label :titulo,"Título", :class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_field :titulo, :class=>"form-control" ,:placeholder=>"Nombre de emprendimiento"%>
        </div>
      </div>
    </div>

    <div class="form-group">
      <div class="field">
        <%= f.label :descripcion,"Descripción", :class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_area :descripcion, :class=>"form-control",:placeholder=>"Descripción de emprendimiento"%>
        </div>
      </div>
    </div>

    <div class="form-group">
      <div class="field">
        <%= f.label :etiquetas,"Etiquetas", :class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_area :etiquetas, :class=>"form-control" ,:placeholder=>"Agregar etiquetas o palabras clave"%>
        </div>
      </div>
    </div>
  </div>

  <div class="col-md-6">

<div class="form-group">
      <div class="field">
        <%= f.label :correo, "Correo Electronico", :class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_field :correo, :class=>"form-control" ,:placeholder=>"Dirección de correo Electronico"%>
        </div>
      </div>
    </div>

    <div class="form-group">
      <div class="field">
        <%= f.label :pweb, "Página web", :class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_field :pweb, :class=>"form-control",:placeholder=>"Url página web" %>
        </div>
      </div>
    </div>

    <div class="form-group">
      <div class="field">
        <%= f.label :direccion,"Dirección", :class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_field :direccion, :class=>"form-control" ,:placeholder=>"Dirección o lugar"%>
        </div>
      </div>
    </div>

    <div class="form-group">
      <div class="field">
        <%= f.label :ihoraio, "Horario Inicio" ,:class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_field :ihoraio, :class=>"form-control",:placeholder=>"Hora inicio de atención" %>
        </div>
      </div>
    </div>

    <div class="form-group">
      <div class="field">
        <%= f.label :fhorario, "Horario Fin", :class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_field :fhorario, :class=>"form-control" ,:placeholder=>"Hora fin de atención"%>
        </div>
      </div>
    </div>
    <div class="form-group">
      <div class="field">
        <%= f.label :telefono, "Teléfono",:class=>"col-lg-2 control-label" %><br>
        <div class="col-lg-10">
        <%= f.text_field :telefono, :class=>"form-control",:placeholder=>"Número de teléfono" %>
        </div>
      </div>
    </div>

  </div>

  </fieldset>
 <!--fin de form--> 
</div>
</div>






    


  
 



 



